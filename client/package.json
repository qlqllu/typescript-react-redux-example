{
  "name": "typescript-react-redux-starter-client",
  "version": "0.1.0",
  "description": "A boiler-plate setup for TypeScript/React/Redux applications",
  "license": "MIT",
  "engines": {
    "node": "6"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "clean": "rimraf builder/",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --progress",
    "server:prod": "cross-env NODE_ENV=production webpack-dev-server --progress",
    "postinstall": "npm run build",
    "test": "karma start --singleRun true",
    "test:watch": "karma start --singleRun false",
    "posttest": "istanbul check-coverage --statement 75 --branch 50 --function 50"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "starter",
    "seed"
  ],
  "dependencies": {
    
  },
  "devDependencies": {
    "react": "^15.3.0",
