    "redux": "^3.6.0",
    "redux-form": "^5.3.2",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^2.6.0",
    "redux-thunk": "^2.1.0",
    "classnames": "^2.2.3",
    "immutable": "^3.8.1",
    "whatwg-fetch": "^1.0.0",


    "ts-helpers": "^1.1.1",
    "typescript": "^2.0.8",
    "cross-env": "^2.0.0",
    "rimraf": "^2.4.3",
    "@types/classnames": "0.0.31",
    "@types/react": "^0.14.47",
    "@types/react-dom": "^0.14.18",
    "@types/react-router": "^2.0.38",
    "@types/react-router-redux": "^4.0.34",
    "@types/redux-thunk": "^2.1.31",
    "@types/whatwg-fetch": "0.0.32",
    "@types/node": "6.0.46",


    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.0",
    "webpack-split-by-path": "0.0.10",
