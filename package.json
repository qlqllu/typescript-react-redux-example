{
  "name": "typescript-react-redux-starter",
  "version": "0.1.0",
  "description": "A boiler-plate setup for TypeScript/React/Redux applications",
  "license": "MIT",
  "engines": {
    "node": "6"
  },
  "scripts": {
    "lint": "npm run lint-ts && npm run lint-js && npm run lint-css",
    "lint-css": "stylelint \"./client/src/**/*.css\" --config client/.stylelintrc",
    "lint-ts": "tslint -e \"**/node_modules/**\" \"**/*.tsx\" \"**/*.ts\" ",
    "lint-js": "eslint \"**/*.js\" --ignore-path .gitignore",
    "pretest": "npm run lint",
    "test": "karma start --singleRun true",
    "test:watch": "karma start --singleRun false",
    "posttest": "istanbul check-coverage --statement 75 --branch 50 --function 50"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "starter",
    "seed"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/qlqllu/typescript-react-redux-starter.git"
  },
  "bugs": {
    "url": "https://github.com/qlqllu/rangle-starter/issues"
  },
  "dependencies": {
  },
  "devDependencies": {
    "stylelint": "^7.5.0",
    "tslint": "^3.7.4",
    "eslint": "^3.2.0"
  }
}